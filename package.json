{
  "name": "vue-sample-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "test:e2e": "vue-cli-service test:e2e",
    "test:unit": "vue-cli-service test:unit"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.27",
    "@fortawesome/free-brands-svg-icons": "^5.12.1",
    "@fortawesome/free-regular-svg-icons": "^5.12.1",
    "@fortawesome/free-solid-svg-icons": "^5.12.1",
    "@fortawesome/vue-fontawesome": "^0.1.9",
    "@sentry/browser": "^5.13.2",
    "@sentry/integrations": "^5.13.2",
    "amazon-cognito-identity-js": "^3.2.5",
    "element-ui": "^2.13.0",
    "is_js": "^0.9.0",
    "js-cookie": "^2.2.1",
    "lodash": "4.17.21",
    "moment": "^2.24.0",
    "register-service-worker": "^1.6.2",
    "sprintf-js": "^1.1.2",
    "text-encoding": "^0.7.0",
    "vue": "^2.6.11",
    "vue-flatpickr-component": "^8.1.5",
    "vue-router": "^3.1.6",
    "vuex": "^3.1.2",
    "vuex-persistedstate": "^2.7.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.2.3",
    "@vue/cli-plugin-e2e-cypress": "^4.2.3",
    "@vue/cli-plugin-eslint": "^4.2.3",
    "@vue/cli-plugin-pwa": "^4.2.3",
    "@vue/cli-plugin-unit-jest": "^4.2.3",
    "@vue/cli-service": "^4.2.3",
    "@vue/eslint-config-airbnb": "^5.0.2",
    "@vue/test-utils": "^1.0.0-beta.31",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-vue": "^6.2.1",
    "fibers": "^4.0.2",
    "flush-promises": "^1.0.2",
    "mocha": "^7.1.0",
    "mochawesome": "^5.0.0",
    "mochawesome-merge": "^4.0.2",
    "sass": "^1.26.2",
    "sass-loader": "^8.0.2",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/airbnb",
      "prettier"
    ],
    "rules": {
      "no-continue": "off",
      "no-underscore-dangle": [
        2,
        {
          "allowAfterThis": true,
          "allowAfterSuper": true
        }
      ]
    },
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "overrides": [
      {
        "files": [
          "*.spec.js"
        ],
        "rules": {
          "no-unused-expressions": "off",
          "no-underscore-dangle": "off"
        }
      }
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.vue$": "vue-jest",
      ".+\\.(css|styl|less|sass|scss|svg|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "^.+\\.jsx?$": "babel-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testMatch": [
      "**/tests/unit/**/*.spec.(js|jsx|ts|tsx)|**/__tests__/*.(js|jsx|ts|tsx)"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(babel-jest|jest-vue-preprocessor|flatpickr)/)"
    ],
    "testURL": "http://localhost/",
    "cache": false,
    "globals": {
      "SENTRY_ENDPOINT": "https://xxxxxxxxxxxxxxxxxxxxxxxxxxx@sentry.io/1234567",
      "DD_DOGS_TOKNEN": "xxxxxxxxxxxxxxxxxxxxxxxxxxx"
    }
  }
}
